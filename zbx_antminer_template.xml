<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2024-03-06T12:00:00Z</date>
    <groups>
        <group>
            <name>Templates/Mining</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Antminer S19k Pro</template>
            <name>Template Antminer S19k Pro</name>
            <description>Template for monitoring Antminer S19k Pro ASIC miner</description>
            <groups>
                <group>
                    <name>Templates/Mining</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>General</name>
                </application>
                <application>
                    <name>Chains</name>
                </application>
                <application>
                    <name>Fans</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Chain discovery</name>
                    <type>0</type>
                    <key>antminer.discovery[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},chains]</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Chain {#CHAINID} temperature</name>
                            <type>10</type>
                            <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.chain.{#CHAINID}.temp_chip]</key>
                            <delay>1m</delay>
                            <units>Â°C</units>
                            <applications>
                                <application>
                                    <name>Chains</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;85</expression>
                                    <name>Chain {#CHAINID} temperature too high</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Chain {#CHAINID} hashrate</name>
                            <type>10</type>
                            <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.chain.{#CHAINID}.rate_real]</key>
                            <delay>1m</delay>
                            <units>GH/s</units>
                            <applications>
                                <application>
                                    <name>Chains</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Chain {#CHAINID} frequency</name>
                            <type>10</type>
                            <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.chain.{#CHAINID}.freq_avg]</key>
                            <delay>1m</delay>
                            <units>MHz</units>
                            <applications>
                                <application>
                                    <name>Chains</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Chain {#CHAINID} hardware errors</name>
                            <type>10</type>
                            <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.chain.{#CHAINID}.hw]</key>
                            <delay>1m</delay>
                            <applications>
                                <application>
                                    <name>Chains</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Fan discovery</name>
                    <type>0</type>
                    <key>antminer.discovery[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},fans]</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Fan {#FANID} speed</name>
                            <type>10</type>
                            <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.fan.{#FANID}]</key>
                            <delay>1m</delay>
                            <units>RPM</units>
                            <applications>
                                <application>
                                    <name>Fans</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;3000</expression>
                                    <name>Fan {#FANID} speed too low</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
            <items>
                <item>
                    <name>Miner uptime</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.elapsed]</key>
                    <delay>5m</delay>
                    <units>s</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>5s hashrate</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.rate_5s]</key>
                    <delay>1m</delay>
                    <units>GH/s</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>30m hashrate</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.rate_30m]</key>
                    <delay>1m</delay>
                    <units>GH/s</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Average hashrate</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.rate_avg]</key>
                    <delay>1m</delay>
                    <units>GH/s</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Hardware error rate</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.hwp_total]</key>
                    <delay>1m</delay>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Frequency level</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.freq-level]</key>
                    <delay>1m</delay>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Operating mode</name>
                    <type>10</type>
                    <key>antminer.get[{HOST.CONN},{$MINER.PORT},{$MINER.USER},{$MINER.PASS},STATS.miner-mode]</key>
                    <delay>1m</delay>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$MINER.PORT}</macro>
                    <value>80</value>
                </macro>
                <macro>
                    <macro>{$MINER.USER}</macro>
                    <value>root</value>
                </macro>
                <macro>
                    <macro>{$MINER.PASS}</macro>
                    <value>root</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
